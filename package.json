{
  "name": "loki",
  "version": "2.7.4",
  "type": "commonjs",
  "productName": "Lowkey Media Viewer",
  "description": "A Media library for quick curation.",
  "keywords": [],
  "homepage": "https://github.com/stevecastle/loki#readme",
  "bugs": {
    "url": "https://github.com/stevecastle/loki/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stevecastle/loki.git"
  },
  "license": "MIT",
  "author": {
    "name": "Callsign Media LLC",
    "email": "dream@lowkeyviewer.com",
    "url": "https://lowkeyviewer.com"
  },
  "main": "./src/main/main.ts",
  "scripts": {
    "build": "concurrently \"npm run build:main\" \"npm run build:renderer\"",
    "build:main": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.main.prod.ts",
    "build:renderer": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.renderer.prod.ts",
    "postinstall": "ts-node .erb/scripts/check-native-dep.js && electron-builder install-app-deps && cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.renderer.dev.dll.ts",
    "lint": "cross-env NODE_ENV=development eslint . --ext .js,.jsx,.ts,.tsx",
    "package": "node ./.erb/scripts/check-binaries.js && ts-node ./.erb/scripts/prebuild-cleanup.js && ts-node ./.erb/scripts/clean.js dist && npm run build && electron-builder build --publish never",
    "prepare": "husky install",
    "rebuild": "electron-rebuild --parallel --types prod,dev,optional --module-dir release/app",
    "start": "ts-node ./.erb/scripts/check-port-in-use.js && npm run start:renderer",
    "start:main": "cross-env NODE_ENV=development electronmon -r tsx .",
    "start:preload": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.preload.dev.ts",
    "start:renderer": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack serve --config ./.erb/configs/webpack.config.renderer.dev.ts",
    "test": "npm run build && jest",
    "test:server": "cd media-server && go test ./..."
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "cross-env NODE_ENV=development eslint --cache"
    ],
    "*.json,.{eslintrc,prettierrc}": [
      "prettier --ignore-path .eslintignore --parser json --write"
    ],
    "*.{css,scss}": [
      "prettier --ignore-path .eslintignore --single-quote --write"
    ],
    "*.{html,md,yml}": [
      "prettier --ignore-path .eslintignore --single-quote --write"
    ]
  },
  "browserslist": [],
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": [
          ".prettierrc",
          ".eslintrc"
        ],
        "options": {
          "parser": "json"
        }
      }
    ]
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "release/app/node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/.erb/mocks/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "./.erb/scripts/check-build-exists.ts"
    ],
    "testEnvironment": "jsdom",
    "testEnvironmentOptions": {
      "url": "http://localhost/"
    },
    "testPathIgnorePatterns": [
      "release/app/dist"
    ],
    "transform": {
      "\\.(ts|tsx|js|jsx)$": "ts-jest"
    }
  },
  "dependencies": {
    "@react-hooks-library/core": "^0.5.1",
    "@rehooks/component-size": "^1.0.3",
    "@tanstack/react-query": "^4.42.0",
    "@tanstack/react-virtual": "^3.13.16",
    "@xstate/react": "^3.2.1",
    "blob": "^0.1.0",
    "electron-debug": "^3.2.0",
    "electron-is": "^3.0.0",
    "electron-log": "^5.4.3",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.6.2",
    "js-base64": "^3.7.8",
    "lodash": "^4.17.21",
    "lodash.memoize": "^4.1.2",
    "moment": "^2.29.4",
    "natural-compare": "^1.4.0",
    "natural-compare-lite": "^1.4.0",
    "path-browserify": "^1.0.1",
    "pngjs": "^7.0.0",
    "react": "^18.3.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.3.1",
    "react-indiana-drag-scroll": "^3.0.3-alpha",
    "react-loading-skeleton": "^3.5.0",
    "react-resizable-panels": "^4.2.0",
    "react-router-dom": "^6.30.2",
    "react-scroll-ondrag": "^3.0.1",
    "react-tooltip": "^5.30.0",
    "readdir-enhanced": "^6.0.4",
    "shuffle-array": "^1.0.1",
    "through2": "^4.0.2",
    "tiny-invariant": "^1.3.3",
    "usehooks-ts": "^2.16.0",
    "workerpool": "^10.0.1",
    "xstate": "^4.37.2"
  },
  "devDependencies": {
    "@electron/notarize": "^3.1.1",
    "@electron/rebuild": "^4.0.2",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.6.2",
    "@svgr/webpack": "^7.0.0",
    "@teamsupercell/typings-for-css-modules-loader": "^2.5.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.3.1",
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/jest": "^29.5.14",
    "@types/lodash.throttle": "^4.1.7",
    "@types/moment": "^2.13.0",
    "@types/natural-compare": "^1.4.1",
    "@types/natural-compare-lite": "^1.4.0",
    "@types/node": "18.15.9",
    "@types/path-browserify": "^1.0.3",
    "@types/pngjs": "^6.0.5",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@types/react-test-renderer": "^18.3.1",
    "@types/shuffle-array": "^1.0.2",
    "@types/terser-webpack-plugin": "^5.0.4",
    "@types/through2": "^2.0.38",
    "@types/webpack-bundle-analyzer": "^4.7.0",
    "@types/workerpool": "^6.4.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "browserslist-config-erb": "^0.0.3",
    "chalk": "4.1.2",
    "concurrently": "^9.2.1",
    "core-js": "^3.47.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.11.0",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "detect-port": "^2.1.0",
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12",
    "electron-devtools-installer": "^3.2.0",
    "electronmon": "^2.0.4",
    "eslint": "^8.36.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-erb": "^4.0.6",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-import-resolver-typescript": "^3.4.1",
    "eslint-import-resolver-webpack": "^0.13.10",
    "eslint-plugin-compat": "^4.1.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.6.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.5",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^16.2.7",
    "mini-css-extract-plugin": "^2.9.4",
    "prettier": "2.8.7",
    "react-refresh": "^0.14.0",
    "react-test-renderer": "^18.3.1",
    "rimraf": "^6.1.2",
    "sass": "^1.97.1",
    "sass-loader": "^13.2.1",
    "style-loader": "^3.3.2",
    "terser-webpack-plugin": "^5.3.16",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "tsx": "^4.21.0",
    "typescript": "5.0.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.104.1",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-merge": "^5.8.0"
  },
  "build": {
    "productName": "Lowkey Media Viewer",
    "appId": "org.csm.LowKey",
    "asar": true,
    "asarUnpack": [
      "**\\*.{node,dll}",
      "**\\image-processing-worker.js"
    ],
    "files": [
      "dist",
      "node_modules",
      "package.json"
    ],
    "afterSign": ".erb/scripts/notarize.js",
    "mac": {
      "target": {
        "target": "default",
        "arch": [
          "arm64",
          "x64"
        ]
      },
      "type": "distribution",
      "hardenedRuntime": true,
      "entitlements": "assets/entitlements.mac.plist",
      "entitlementsInherit": "assets/entitlements.mac.plist",
      "binaries": [
        "src/main/resources/bin/darwin/ffmpeg",
        "src/main/resources/bin/darwin/ffprobe"
      ],
      "icon": "assets/icon.png",
      "gatekeeperAssess": false,
      "extraResources": [
        {
          "from": "./src/main/resources/bin/darwin/",
          "to": "bin",
          "filter": [
            "**/*"
          ]
        }
      ]
    },
    "fileAssociations": [
      {
        "ext": "jpg",
        "name": "Jpg Image File",
        "role": "Viewer"
      },
      {
        "ext": "jpeg",
        "name": "Jpeg Image File",
        "role": "Viewer"
      },
      {
        "ext": "jfif",
        "name": "Jfif Image File",
        "role": "Viewer"
      },
      {
        "ext": "webp",
        "name": "WebP Image File",
        "role": "Viewer"
      },
      {
        "ext": "png",
        "name": "PNG Image File",
        "role": "Viewer"
      },
      {
        "ext": "gif",
        "name": "Gif Image File",
        "role": "Viewer"
      },
      {
        "ext": "webm",
        "name": "Webm Video File",
        "role": "Viewer"
      },
      {
        "ext": "flv",
        "name": "Flv Video File",
        "role": "Viewer"
      },
      {
        "ext": "mp4",
        "name": "Mp4 Video File",
        "role": "Viewer"
      },
      {
        "ext": "mov",
        "name": "Mov Video File",
        "role": "Viewer"
      },
      {
        "ext": "m4v",
        "name": "M4v Video File",
        "role": "Viewer"
      },
      {
        "ext": "m4a",
        "name": "M4a Audio File",
        "role": "Viewer"
      },
      {
        "ext": "mkv",
        "name": "Mkv Video File",
        "role": "Viewer"
      }
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "assets/icon.png",
      "extraResources": [
        {
          "from": "./src/main/resources/bin/win32/",
          "to": "bin",
          "filter": [
            "**/*"
          ]
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Development",
      "extraResources": [
        {
          "from": "./src/main/resources/bin/linux/",
          "to": "bin",
          "filter": [
            "**/*"
          ]
        }
      ]
    },
    "directories": {
      "app": "release/app",
      "buildResources": "assets",
      "output": "release/build"
    },
    "extraResources": [
      "./assets/**"
    ],
    "publish": {
      "provider": "github",
      "owner": "stevecastle",
      "repo": "loki"
    }
  },
  "electronmon": {
    "patterns": [
      "!**/**",
      "src/main/**"
    ],
    "logLevel": "quiet"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
